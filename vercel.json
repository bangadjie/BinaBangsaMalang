{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    },
    "*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/BinaBangsaMalang.html"
    },
    {
      "src": "/Admin",
      "dest": "/Admin.php"
    },
    {
      "src": "/ExportExcel",
      "dest": "/ExportExcel.php"
    },
    {
      "src": "/Proses",
      "dest": "/Proses.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}